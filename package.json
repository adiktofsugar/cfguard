        "@preact/preset-vite": "^2.10.2",
        "@preact/signals": "^2.3.1",
        "@tsconfig/node22": "^22.0.2",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.17.2",
        "@types/qrcode": "^1.5.5",
        "@vitejs/plugin-legacy": "^7.2.1",
        "concurrently": "^9.2.0",
        "minimist": "^1.2.8",
        "playwright": "^1.55.0",
        "preact": "^10.19.3",
        "tsx": "^4.20.4",
        "typescript": "^5.9.2",
        "vite": "^7.1.4",
        "wrangler": "^4.31.0"
    }

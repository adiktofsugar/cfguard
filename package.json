        "@fortawesome/fontawesome-svg-core": "^7.0.0",
        "@fortawesome/free-solid-svg-icons": "^7.0.0",
        "@picocss/pico": "^2.1.1",
        "@preact/signals": "^2.3.1",
        "express": "^4.21.2",
        "hono": "^4.9.5",
        "js-logger": "^1.6.1",
        "mobx": "^6.13.7",
        "qrcode": "^1.5.4",
        "qrcode-generator": "^2.0.4",
        "whatwg-url": "^15.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.0",
        "@cloudflare/vite-plugin": "^1.12.3",
        "@cloudflare/workers-types": "^4.20250831.0",

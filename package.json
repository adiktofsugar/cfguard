{
    "name": "cfguard",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently -n vite,wrangler \"vite build --watch\" \"wrangler dev\"",
        "build": "vite build",
        "deploy": "vite build && wrangler deploy",
        "log": "wrangler tail",
        "test": "playwright test",
        "types": "wrangler types worker/worker-types.d.ts",
        "lint": "concurrently -gr npm:lint:*",
        "lint:biome": "biome check",
        "lint:tsc": "tsc --build",
        "fix": "biome check --fix --unsafe .",
        "build:old": "tsx scripts/build.mts",
        "dev:old": "tsx scripts/build.mts --dev"
    },
    "author": "",
    "license": "ISC",
    "description": "",
